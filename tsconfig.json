{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"target": "ES6",
		"module": "commonjs",
		"moduleResolution": "Node",
		"jsx": "react-native",
		"lib": [
			"dom",
			"esnext"
		],
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./*"
			]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts"
	],
	"exclude": [
		"node_modules"
	]
}